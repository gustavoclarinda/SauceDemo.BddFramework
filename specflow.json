{
  "language": {
    "feature": "en-US"
  },
  "unitTestProvider": {
    "name": "NUnit"
  },
  "generator": {
    "allowDebugGeneratedFiles": false,
    "allowRowTests": true,
    "path": "Features",
    "addNonParallelizableMarkerForTags": [
      "nonparallel"
    ]
  },
  "stepAssemblies": [],
  "specFlow": {
    "stepDefinitions": {
      "style": "CucumberExpressions"
    },
    "plugins": [
      {
        "name": "Allure",
        "type": "GeneratorAndRuntime"
      }
    ]
  }
}